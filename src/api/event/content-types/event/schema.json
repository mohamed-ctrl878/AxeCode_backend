{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "discription": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date": {
      "type": "datetime"
    },
    "onsite": {
      "type": "boolean",
      "default": true
    },
    "live_streaming": {
      "type": "boolean"
    },
    "location": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "speakers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speaker.speaker",
      "mappedBy": "event"
    },
    "event_activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-activity.event-activity",
      "mappedBy": "event"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    },
    "scanners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::scanner.scanner",
      "mappedBy": "event"
    },
    "tags": {
      "type": "json"
    },
    "engagement_score": {
      "type": "integer",
      "default": 0
    }
  }
}