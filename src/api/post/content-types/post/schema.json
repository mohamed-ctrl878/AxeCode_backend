{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "likes": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "caption": {
      "type": "blocks"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "postbox": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::postbox.postbox",
      "inversedBy": "posts"
    },
    "title": {
      "type": "string"
    }
  }
}
