{
  "kind": "collectionType",
  "collectionName": "problems",
  "info": {
    "singularName": "problem",
    "pluralName": "problems",
    "displayName": "Problem",
    "description": "Algorithm problems for practice"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard"
      ],
      "default": "easy",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "constraints": {
      "type": "text"
    },
    "examples": {
      "type": "json"
    },
    "hints": {
      "type": "json"
    },
    "functionName": {
      "type": "string",
      "required": true
    },
    "functionParams": {
      "type": "json"
    },
    "returnType": {
      "type": "string"
    },
    "timeLimit": {
      "type": "integer",
      "default": 2000
    },
    "memoryLimit": {
      "type": "integer",
      "default": 256000
    },
    "public": {
      "type": "boolean",
      "default": false
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "problem_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::problem-type.problem-type",
      "inversedBy": "problems"
    },
    "test_cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::test-case.test-case",
      "mappedBy": "problem"
    },
    "submissions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::submission.submission",
      "mappedBy": "problem"
    },
    "code_templates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::code-template.code-template",
      "mappedBy": "problem"
    },
    "tags": {
      "type": "json"
    },
    "engagement_score": {
      "type": "integer",
      "default": 0
    }
  }
}