{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "courses",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "picture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "weeks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::week.week",
      "mappedBy": "course"
    },
    "problem_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::problem-type.problem-type",
      "inversedBy": "courses"
    },
    "course_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course-type.course-type",
      "mappedBy": "courses"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "Rasy",
        "Medium",
        "Advanced"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "tags": {
      "type": "json"
    },
    "engagement_score": {
      "type": "integer",
      "default": 0
    }
  }
}