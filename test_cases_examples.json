{
  "info": {
    "endpoints": {
      "public": "http://localhost:1338/api/code-execution",
      "admin": "http://localhost:1338/api/code-execution-admin"
    }
  },
  "examples": [
    {
      "description": "اختبار دالة تكرار النص",
      "request": {
        "language": "cpp",
        "code": "string repeatString(string str, int times) { string result = \"\"; while (times-- > 0) result += str; return result; }",
        "testCases": [
          {
            "id": 1,
            "inputs": ["hello", 3],
            "inputTypes": ["string", "int"]
          },
          {
            "id": 2,
            "inputs": ["abc", 2],
            "inputTypes": ["string", "int"]
          }
        ],
        "functionName": "repeatString",
        "functionReturnType": "string",
        "expected": ["hellohellohello", "abcabc"]
      }
    },
    {
      "description": "اختبار دالة حساب المتوسط",
      "request": {
        "language": "cpp",
        "code": "double average(vector<int> numbers) { if(numbers.empty()) return 0; double sum = 0; for(int num : numbers) sum += num; return sum / numbers.size(); }",
        "testCases": [
          {
            "id": 1,
            "inputs": [[1, 2, 3, 4, 5]],
            "inputTypes": ["vector<int>"]
          },
          {
            "id": 2,
            "inputs": [[10, 20, 30]],
            "inputTypes": ["vector<int>"]
          }
        ],
        "functionName": "average",
        "functionReturnType": "double",
        "expected": [3.0, 20.0]
      }
    },
    {
      "description": "اختبار دالة عكس مصفوفة",
      "request": {
        "language": "cpp",
        "code": "vector<int> reverseArray(vector<int> arr) { reverse(arr.begin(), arr.end()); return arr; }",
        "testCases": [
          {
            "id": 1,
            "inputs": [[1, 2, 3, 4, 5]],
            "inputTypes": ["vector<int>"]
          },
          {
            "id": 2,
            "inputs": [[9, 8, 7]],
            "inputTypes": ["vector<int>"]
          }
        ],
        "functionName": "reverseArray",
        "functionReturnType": "vector<int>",
        "expected": [[5, 4, 3, 2, 1], [7, 8, 9]]
      }
    }
  ]
}
